{
  "name": "@iomechs/react-native-location-view",
  "version": "0.0.2",
  "description": "A package to help pick user location with autocomplete and current location support. Uses react-native-maps",
  "main": "index.js",
  "author": "Hunaid Hassan <hhunaid@gmail.com>",
  "license": "MIT",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IOMechs/react-native-location-view"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint --fix",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint && npm run changelog",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "changelog": "./node_modules/.bin/conventional-changelog -p angular -k package.json -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "keywords": [
    "autocomplete",
    "google",
    "places",
    "react-component",
    "react-native",
    "ios",
    "android",
    "location",
    "location-picker"
  ],
  "bugs": {
    "url": "https://github.com/IOMechs/react-native-location-view"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "prop-types": "^15.6.0",
    "react-native-simple-events": "^1.0.1"
  },
  "files": [
    "index.js",
    "src/*",
    "utils/*"
  ],
  "peerDependencies": {
    "@react-native-community/geolocation": "^2.0.0",
    "react-native": ">= 0.50",
    "react-native-maps": ">= 0.19.0",
    "react-native-vector-icons": "^4.4.3"
  },
  "devDependencies": {
    "conventional-changelog-cli": "^2.0.34",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  }
}
